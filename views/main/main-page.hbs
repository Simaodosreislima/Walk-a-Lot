<div class="main-page">
  <h1>Hi {{firstName}}!</h1>

  {{!-- <p>Hello
    {{firstName}}
    {{lastName}}, you're username is
    {{username}}
    and you're email is
    {{email}}</p> --}}
  <button><a href='/cards/create'>New Walk</a></button>

  <div class="main-page-container">

    {{#each walkCards}}
    <div class="card">
      <div class="card-image">
        <img src='{{cardImageUrl}}' alt='{{title}}' />
      </div>
      <h5>{{title}}</h5>
      <span>{{description}}</span>
      <button><a href='/main/{{id}}/details'>View Details</a></button>
      {{!-- <form action='/main/{{_id}}/delete' method='post'>
        <button>Delete walk</button>

      </form> --}}

    </div>
    {{/each}}
  </div>
</div>