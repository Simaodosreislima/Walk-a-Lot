<div class='container'>
  <div id='map'></div>

  <aside>
    <form action='/map/create' method='post'>
      <h1 style='color: white'>Start Point</h1>
      <input type='text' name='latitude' id='lat' />
      <input type='text' name='longitude' id='lng' />
      <button class='profileBtn' type='submit'>Submit</button>
    </form>
    <form action='/map/create' method='post'>
      <h1 style='color: white'>End Point</h1>
      <input type='text' name='latitude' id='lat' />
      <input type='text' name='longitude' id='lng' />
      <button class='profileBtn' type='submit'>Submit</button>
    </form>

    <button class='mapBtn' type='submit'>GET WALK ROUTE</button>

